{
   "name": "service",
   "version": "1.0.0",
   "description": "",
   "main": "index.js",
   "scripts": {
      "dev": "tsc && node ./build/server.js",
      "build": "./remove-erroneous-files.sh && npx -yes node-git-info && tsc",
      "pre-commit": "./remove-intermediate-files.sh && npm ci && npm run build && npm run test:coverage",
      "test": "NODE_ENV=local npm run test:micro && npm run test:integration",
      "test:coverage": "jest --coverage --passWithNoTests --testMatch '**/?(*.)+(micro|integration).ts'",
      "test:integration": "NODE_ENV=test jest --passWithNoTests --testMatch '/work/service/src/**/*.integration.ts'",
      "test:micro": "jest --passWithNoTests --testMatch '/work/service/src/**/*.micro.ts'",
      "upgrade-all": "npx -yes -p npm-check-updates ncu -u && npm install"
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "dependencies": {
      "axios": "1.4.0",
      "body-parser": "1.20.2",
      "cors": "2.8.5",
      "express": "4.18.2",
      "express-actuator": "1.8.4",
      "inversify": "6.0.1",
      "inversify-binding-decorators": "4.0.0",
      "swagger-ui-express": "5.0.0",
      "ts-luxon": "4.3.2",
      "tsoa": "5.1.1",
      "winston": "3.10.0"
   },
   "devDependencies": {
      "@jest-mock/express": "2.0.2",
      "@types/express": "4.17.17",
      "@types/jest": "29.5.3",
      "@types/supertest": "2.0.12",
      "jest": "29.6.2",
      "nodemon": "3.0.1",
      "supertest": "6.3.3",
      "ts-jest": "29.1.1",
      "ts-node": "10.9.1",
      "tslib": "2.6.1",
      "typescript": "5.1.6"
   }
}
